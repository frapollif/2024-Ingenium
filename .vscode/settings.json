{
 
"latex-workshop.latex.outDir": "%DIR%/tmp",

"latex-workshop.latex.recipes": [
    {
      "name": "pdflatex-linux",
      "tools": [
        "pdflatex",
        "copypdf-linux",
      ]
    },
    {
      "name": "pdflatex-win",
      "tools": [
        "pdflatex",
        "copypdf-win",
      ]
    },
],

"latex-workshop.latex.tools": [
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-output-directory=%OUTDIR%",
        "--shell-escape",
        "%DOC%"
      ],
      "env": {}
    },
    {
        "name": "copypdf-linux",
        "command": "cp",
        "args": [
          "%OUTDIR_W32%/%DOCFILE%.pdf",
          "%DIR_W32%/"
        ]
      },

      {
        "name": "copypdf-win",
        "command": "xcopy",
        "args": [
          "%OUTDIR/%DOCFILE%.pdf",
          "%DIR/"
        ]
      },
],
"latex-workshop.latex.autoBuild.run": "never",
"git.confirmSync": false

}


